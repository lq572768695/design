<!-- /.col -->
<div class="col-md-12">
  <div class="box box-primary">
    <div class="box-header with-border">
      <h3 class="box-title">图片列表</h3>
    </div>
    <div class="box-header with-border">
      <input type="text" class="form-control" placeholder="相册名称模糊搜索" style="width:35%;float:left;">
      <button type="" class="btn btn-primary " style="margin-left:10px;">搜索</button>
      <button type="" id="addimage" class="btn btn-primary" style="float:right;margin-right:10px;">添加图片</button>
    </div>
    <div class="box-footer">
      <ul class="mailbox-attachments clearfix">
        <% picturelist.forEach(function(picture){ %>
          <li>
            <span class="mailbox-attachment-icon has-img"><img src="<%=picture.path%>" alt="Attachment"></span>
            <div class="mailbox-attachment-info">
              <a href="#" class="mailbox-attachment-name"><i class="fa fa-camera"></i></a>
              <a id="delete_photospic" imgid="<%=picture.id%>" href="#" class="mailbox-attachment-name" style="float:right;"><i class="fa  fa-trash"></i></a>
              <span class="mailbox-attachment-size"  style="margin-top:5px;">
                <%=picture.size%>
                <!-- <a href="#" class="btn btn-default btn-xs pull-right">3</a> -->
              </span>
            </div>
          </li>
        <% }) %>
      </ul>
    </div>
  </div>
  <!-- /. box -->
</div>
<!-- /.col -->